pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('Build'){
            steps{
                echo 'Building code'
            }
        }
        
        stage('Testing'){
            steps{
                echo 'Testing Code'
            }
        }
        
        stage('Final'){
            steps{
                echo 'Finalize'
            }
        }
    }
}
